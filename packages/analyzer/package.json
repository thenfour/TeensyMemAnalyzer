{
    "name": "@teensy-mem-explorer/analyzer",
    "version": "0.1.0",
    "description": "Static analysis core for Teensy memory usage.",
    "license": "MIT",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.js",
            "import": "./dist/index.js"
        },
        "./analysis/reports/teensy-size": {
            "types": "./dist/analysis/reports/teensy-size.d.ts",
            "require": "./dist/analysis/reports/teensy-size.js",
            "import": "./dist/analysis/reports/teensy-size.js"
        },
        "./analysis/address-resolver": {
            "types": "./dist/analysis/address-resolver.d.ts",
            "require": "./dist/analysis/address-resolver.js",
            "import": "./dist/analysis/address-resolver.js"
        }
    },
    "scripts": {
        "build": "tsc -b",
        "clean": "rimraf dist",
        "lint": "eslint \"src/**/*.{ts,tsx}\""
    },
    "dependencies": {
        "ajv": "^8.12.0"
    },
    "devDependencies": {
        "@types/node": "^20.10.6",
        "rimraf": "^5.0.5"
    }
}