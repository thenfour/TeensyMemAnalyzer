{
    "name": "@teensy-mem-explorer/viewer",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "description": "Interactive visualization for Teensy memory analysis results.",
    "scripts": {
        "dev": "concurrently \"yarn dev:client\" \"yarn dev:server\"",
        "dev:client": "vite",
        "dev:server": "tsx watch src/server/index.ts",
        "build": "tsc -p tsconfig.json --noEmit && vite build && tsc -p tsconfig.server.json",
        "preview": "vite preview",
        "server:start": "node dist/server/index.js",
        "server:build": "tsc -p tsconfig.server.json",
        "lint": "eslint src --ext .ts,.tsx"
    },
    "dependencies": {
        "express": "^4.19.2",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "ws": "^8.18.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/node": "^20.11.30",
        "@types/react": "^18.2.47",
        "@types/react-dom": "^18.2.18",
        "@types/ws": "^8.5.10",
        "@vitejs/plugin-react": "^4.3.1",
        "concurrently": "^8.2.2",
        "eslint": "^8.56.0",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "tsx": "^4.7.1",
        "typescript": "^5.3.3",
        "vite": "^5.0.8"
    }
}